<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation_graph.xml"
    android:label="BottomNavFragment"
    app:startDestination="@id/exploreFragment">

    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.nanicky.devteam.main.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore">
        <action
            android:id="@+id/action_exploreFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_albumSongsFragment"
            app:destination="@id/albumSongsFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_albumsMenuBottomSheetDialogFragment"
            app:destination="@id/albumsMenuBottomSheetDialogFragment"
            app:launchSingleTop="true" >
            <argument android:defaultValue="@id/exploreFragment"
                android:name="popUpTo" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/songsFragment"
        android:name="com.nanicky.devteam.main.songs.SongsFragment"
        android:label="SongsFragment"
        tools:layout="@layout/fragment_base_player">
        <action
            android:id="@+id/action_songsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="0" />
        </action>
        <action
            android:id="@+id/action_songsFragment_to_songsMenuBottomSheetDialogFragment"
            app:destination="@id/songsMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/songsFragment" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/albumsFragment"
        android:name="com.nanicky.devteam.main.albums.AlbumsFragment"
        android:label="AlbumsFragment"
        tools:layout="@layout/fragment_base_player">
        <action
            android:id="@+id/action_albumsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment" />
        <action
            android:id="@+id/action_albumsFragment_to_albumSongsFragment"
            app:destination="@id/albumSongsFragment" />
        <action
            android:id="@+id/action_albumsFragment_to_albumsMenuBottomSheetDialogFragment"
            app:destination="@id/albumsMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/albumsFragment" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/playlistFragment"
        android:name="com.nanicky.devteam.main.playlist.PlaylistFragment"
        android:label="PlaylistFragment"
        tools:layout="@layout/fragment_playlist">
        <action
            android:id="@+id/action_playlistFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="5" />
        </action>
        <action
            android:id="@+id/action_playlistFragment_to_playlistSongsFragment"
            app:destination="@id/playlistSongsFragment" />
        <action
            android:id="@+id/action_playlistFragment_to_writePlaylistDialogFragment"
            app:destination="@id/writePlaylistDialogFragment" />
        <action
            android:id="@+id/action_playlistFragment_to_playlistMenuBottomSheetDialogFragment"
            app:destination="@id/playlistMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/playlistFragment" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.nanicky.devteam.main.search.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_songsMenuBottomSheetDialogFragment"
            app:destination="@id/songsMenuBottomSheetDialogFragment" >
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/searchFragment" />
        </action>
        <action
            android:id="@+id/action_searchFragment_to_albumsMenuBottomSheetDialogFragment"
            app:destination="@id/albumsMenuBottomSheetDialogFragment" >
            <argument android:defaultValue="@id/searchFragment"
                android:name="popUpTo" />
        </action>
        <action
            android:id="@+id/action_searchFragment_to_albumSongsFragment"
            app:destination="@id/albumSongsFragment" />
        <action
            android:id="@+id/action_searchFragment_to_artistAlbumsFragment"
            app:destination="@id/artistAlbumsFragment" />
        <action
            android:id="@+id/action_searchFragment_to_genreSongsFragment"
            app:destination="@id/genreSongsFragment" />
        <action
            android:id="@+id/action_searchFragment_to_genresMenuBottomSheetDialogFragment"
            app:destination="@id/genresMenuBottomSheetDialogFragment" >
            <argument android:defaultValue="@id/searchFragment"
                android:name="popUpTo" />
        </action>
        <action
            android:id="@+id/action_searchFragment_to_playlistSongsFragment"
            app:destination="@id/playlistSongsFragment" />
        <action
            android:id="@+id/action_searchFragment_to_playlistMenuBottomSheetDialogFragment"
            app:destination="@id/playlistMenuBottomSheetDialogFragment" >
            <argument android:defaultValue="@id/searchFragment"
                android:name="popUpTo" />
        </action>
    </fragment>

    <dialog
        android:id="@+id/navigationDialogFragment"
        android:name="com.nanicky.devteam.main.navigation.NavigationDialogFragment"
        android:label="fragment_navigation_dialog"
        tools:layout="@layout/fragment_navigation_dialog">
        <argument
            android:name="origin"
            android:defaultValue="-1"
            app:argType="integer" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_songsFragment"
            app:destination="@id/songsFragment"
            app:popUpTo="@+id/navigationDialogFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_playlistFragment"
            app:destination="@id/playlistFragment"
            app:popUpTo="@+id/navigationDialogFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_artistsFragment"
            app:destination="@id/artistsFragment"
            app:launchSingleTop="false"
            app:popUpTo="@+id/navigationDialogFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_genresFragment"
            app:destination="@id/genresFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_foldersFragment"
            app:destination="@id/foldersFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_self"
            app:destination="@id/navigationDialogFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_shazamFragment"
            app:destination="@id/shazamFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_webFragment"
            app:destination="@id/webFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_favouriteSongsFragment"
            app:destination="@id/favouriteSongsFragment" />
        <action
            android:id="@+id/action_navigationDialogFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </dialog>
    <fragment
        android:id="@+id/albumSongsFragment"
        android:name="com.nanicky.devteam.main.albums.AlbumSongsFragment"
        android:label="albumSongsFragment"
        tools:layout="@layout/fragment_album_songs">
        <argument
            android:name="album"
            app:argType="com.nanicky.devteam.main.albums.Album" />
        <argument
            android:name="transitionName"
            app:argType="string" />
        <action
            android:id="@+id/action_albumSongsFragment_to_albumsMenuBottomSheetDialogFragment"
            app:destination="@id/albumsMenuBottomSheetDialogFragment" >
            <argument android:defaultValue="@id/albumSongsFragment"
                android:name="popUpTo" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/artistsFragment"
        android:name="com.nanicky.devteam.main.artists.ArtistsFragment"
        android:label="fragment_artists"
        tools:layout="@layout/fragment_artists">
        <action
            android:id="@+id/action_artistsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="2" />
        </action>
        <action
            android:id="@+id/action_artistsFragment_to_artistAlbumsFragment"
            app:destination="@id/artistAlbumsFragment"
            app:launchSingleTop="false" />
    </fragment>
    <fragment
        android:id="@+id/artistAlbumsFragment"
        android:name="com.nanicky.devteam.main.artists.ArtistAlbumsFragment"
        android:label="ArtistAlbumsFragment"
        tools:layout="@layout/fragment_artist_albums">
        <action
            android:id="@+id/action_artistAlbumsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment"
            app:launchSingleTop="false" />
        <argument
            android:name="artist"
            app:argType="com.nanicky.devteam.main.artists.Artist" />
        <argument
            android:name="transitionName"
            app:argType="string" />
        <action
            android:id="@+id/action_artistAlbumsFragment_to_albumSongsFragment"
            app:destination="@id/albumSongsFragment" />
        <action
            android:id="@+id/action_artistAlbumsFragment_to_albumsMenuBottomSheetDialogFragment"
            app:destination="@id/albumsMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/artistAlbumsFragment" />
        </action>
    </fragment>
    <dialog
        android:id="@+id/songsMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.songs.SongsMenuBottomSheetDialogFragment"
        android:label="fragment_songs_menu_bottom_sheet_dialog"
        tools:layout="@layout/fragment_songs_menu_bottom_sheet_dialog">
        <action
            android:id="@+id/action_songsMenuBottomSheetDialogFragment_to_addSongsToPlaylistsFragment"
            app:destination="@id/addSongsToPlaylistsFragment"
            app:launchSingleTop="true" />
        <argument
            android:name="popUpTo"
            app:argType="reference" />
        <action
            android:id="@+id/action_songsMenuBottomSheetDialogFragment_to_webFragment"
            app:destination="@id/webFragment" />
    </dialog>
    <fragment
        android:id="@+id/playlistSongsFragment"
        android:name="com.nanicky.devteam.main.playlist.PlaylistSongsFragment"
        android:label="PlaylistSongsFragment"
        tools:layout="@layout/fragment_playlist_songs">
        <argument
            android:name="transitionName"
            app:argType="string" />
        <argument
            android:name="playlist"
            app:argType="com.nanicky.devteam.main.db.playlist.Playlist" />
        <action
            android:id="@+id/action_playlistSongsFragment_to_playlistMenuBottomSheetDialogFragment"
            app:destination="@id/playlistMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/playlistSongsFragment" />
        </action>
    </fragment>
    <dialog
        android:id="@+id/writePlaylistDialogFragment"
        android:name="com.nanicky.devteam.main.playlist.WritePlaylistDialogFragment"
        android:label="fragment_new_playlist_dialog"
        tools:layout="@layout/fragment_write_playlist_dialog">
        <argument
            android:name="playlist"
            android:defaultValue="@null"
            app:argType="com.nanicky.devteam.main.db.playlist.Playlist"
            app:nullable="true" />
    </dialog>
    <dialog
        android:id="@+id/playlistMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.playlist.PlaylistMenuBottomSheetDialogFragment"
        android:label="fragment_playlist_menu_bottom_sheet_dialog"
        tools:layout="@layout/fragment_playlist_menu_bottom_sheet_dialog">
        <argument
            android:name="playlist"
            app:argType="com.nanicky.devteam.main.db.playlist.Playlist" />
        <argument
            android:name="popUpTo"
            app:argType="reference" />
        <action
            android:id="@+id/action_playlistMenuBottomSheetDialogFragment_to_playlistSongsEditorDialogFragment"
            app:destination="@id/playlistSongsEditorDialogFragment"
            app:launchSingleTop="true" />
        <action
            android:id="@+id/action_playlistMenuBottomSheetDialogFragment_to_writePlaylistDialogFragment"
            app:destination="@id/writePlaylistDialogFragment"
            app:launchSingleTop="true" />
    </dialog>
    <dialog
        android:id="@+id/playlistSongsEditorDialogFragment"
        android:name="com.nanicky.devteam.main.playlist.PlaylistSongsEditorDialogFragment"
        android:label="PlaylistSongsEditorDialogFragment"
        tools:layout="@layout/fragment_playlist_songs_editor_dialog">
        <argument
            android:name="playlist"
            app:argType="com.nanicky.devteam.main.db.playlist.Playlist" />
    </dialog>
    <fragment
        android:id="@+id/genresFragment"
        android:name="com.nanicky.devteam.main.genres.GenresFragment"
        android:label="fragment_genres"
        tools:layout="@layout/fragment_genres">
        <action
            android:id="@+id/action_genresFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="4" />
        </action>
        <action
            android:id="@+id/action_genresFragment_to_genreSongsFragment"
            app:destination="@id/genreSongsFragment" />
        <action
            android:id="@+id/action_genresFragment_to_genresMenuBottomSheetDialogFragment"
            app:destination="@id/genresMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/genresFragment" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/genreSongsFragment"
        android:name="com.nanicky.devteam.main.genres.GenreSongsFragment"
        android:label="GenreSongsFragment"
        tools:layout="@layout/fragment_genre_songs">
        <argument
            android:name="genre"
            app:argType="com.nanicky.devteam.main.genres.Genre" />
        <argument
            android:name="transitionName"
            app:argType="string" />
        <action
            android:id="@+id/action_genreSongsFragment_to_genresMenuBottomSheetDialogFragment"
            app:destination="@id/genresMenuBottomSheetDialogFragment"
            app:launchSingleTop="true">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/genreSongsFragment" />
        </action>
    </fragment>
    <dialog
        android:id="@+id/genresMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.genres.GenresMenuBottomSheetDialogFragment"
        android:label="fragment_genres_menu_bottom_sheet_dialog"
        tools:layout="@layout/fragment_genres_menu_bottom_sheet_dialog" >
        <argument
            android:name="genre"
            app:argType="com.nanicky.devteam.main.genres.Genre" />
        <argument
            android:name="popUpTo"
            app:argType="reference" />
        <action
            android:id="@+id/action_genresMenuBottomSheetDialogFragment_to_addSongsToPlaylistsFragment"
            app:destination="@id/addSongsToPlaylistsFragment"
            app:launchSingleTop="true" />
    </dialog>
    <dialog
        android:id="@+id/addSongsToPlaylistsFragment"
        android:name="com.nanicky.devteam.main.playlist.AddSongsToPlaylistsFragment"
        android:label="fragment_add_songs_to_playlists"
        tools:layout="@layout/fragment_add_songs_to_playlists" >
        <argument
            android:name="songId"
            app:nullable="true"
            android:defaultValue="@null"
            app:argType="string" />
        <argument
            android:name="mediaRoot"
            app:nullable="true"
            android:defaultValue="@null"
            app:argType="string" />
        <!--<action
            android:id="@+id/action_addSongsToPlaylistsFragment_to_writePlaylistDialogFragment"
            app:destination="@id/writePlaylistDialogFragment"
            app:launchSingleTop="true" />-->
    </dialog>
    <dialog
        android:id="@+id/albumsMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.albums.AlbumsMenuBottomSheetDialogFragment"
        android:label="fragment_albums_menu_bottom_sheet_dialog"
        tools:layout="@layout/fragment_albums_menu_bottom_sheet_dialog" >
        <action
            android:id="@+id/action_albumsMenuBottomSheetDialogFragment_to_addSongsToPlaylistsFragment"
            app:destination="@id/addSongsToPlaylistsFragment" />
        <argument
            android:name="popUpTo"
            app:argType="reference" />
        <argument
            android:name="album"
            app:argType="com.nanicky.devteam.main.albums.Album" />
    </dialog>
    <fragment
        android:id="@+id/shazamFragment"
        android:name="com.nanicky.devteam.main.shazam.ShazamFragment"
        android:label="ShazamFragment" >
        <action
            android:id="@+id/action_shazamFragment_to_fragmentShazamBottomDialog"
            app:destination="@+id/fragmentShazamBottomDialog" />
        <action
            android:id="@+id/action_shazamFragment_to_fragmentShazamBottomDialog2"
            app:destination="@id/fragmentShazamBottomDialog" />
        <action
            android:id="@+id/action_shazamFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment" />
    </fragment>
    <dialog
        tools:layout="@layout/fragment_shazam_result_dialog"
        android:id="@+id/fragmentShazamBottomDialog"
        android:name="com.nanicky.devteam.main.shazam.FragmentShazamBottomDialog"
        android:label="FragmentShazamBottomDialog" >
        <argument
            android:name="resultTrackJson"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/foldersFragment"
        android:name="com.nanicky.devteam.main.folders.FoldersFragment"
        android:label="fragment_folders"
        tools:layout="@layout/fragment_folders" >
        <action
            android:id="@+id/action_foldersFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="9" />
        </action>
        <action
            android:id="@+id/action_foldersFragment_to_folderSongsFragment"
            app:destination="@id/folderSongsFragment" />
    </fragment>
    <fragment
        android:id="@+id/folderSongsFragment"
        android:name="com.nanicky.devteam.main.folders.FolderSongsFragment"
        android:label="FolderSongsFragment" >
        <action
            android:id="@+id/action_folderSongsFragment_to_folderSongsMenuBottomSheetDialogFragment"
            app:destination="@id/folderSongsMenuBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_folderSongsFragment_to_foldersMenuBottomSheetDialogFragment"
            app:destination="@id/foldersMenuBottomSheetDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/webFragment"
        android:name="com.nanicky.devteam.main.web.WebFragment"
        android:label="WebFragment" />
    <fragment
        android:id="@+id/favouriteSongsFragment"
        android:name="com.nanicky.devteam.main.favourite.FavouriteSongsFragment"
        android:label="FavouriteSongsFragment" >
        <action
            android:id="@+id/action_favouriteSongsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="3" />
        </action>
        <action
            android:id="@+id/action_favouriteSongsFragment_to_songsMenuBottomSheetDialogFragment"
            app:destination="@id/songsMenuBottomSheetDialogFragment">
            <argument
                android:name="popUpTo"
                android:defaultValue="@id/favouriteSongsFragment" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.nanicky.devteam.main.settings.SettingsFragment">
        <action
            android:id="@+id/action_settingsFragment_to_navigationDialogFragment"
            app:destination="@id/navigationDialogFragment">
            <argument
                android:name="origin"
                android:defaultValue="7" />
        </action>
    </fragment>

    <dialog
        android:id="@+id/folderSongsMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.songs.FolderSongsMenuBottomSheetDialogFragment"
        android:label="FolderSongsMenuBottomSheetDialogFragment" >
        <action
            android:id="@+id/action_folderSongsMenuBottomSheetDialogFragment_to_webFragment"
            app:destination="@id/webFragment" />
        <action
            android:id="@+id/action_folderSongsMenuBottomSheetDialogFragment_to_addSongsToPlaylistsFragment"
            app:destination="@id/addSongsToPlaylistsFragment" />
    </dialog>
    <dialog
        android:id="@+id/foldersMenuBottomSheetDialogFragment"
        android:name="com.nanicky.devteam.main.folders.dialog.FoldersMenuBottomSheetDialogFragment"
        android:label="FoldersMenuBottomSheetDialogFragment" >
        <action
            android:id="@+id/action_foldersMenuBottomSheetDialogFragment_to_addSongsToPlaylistsFragment"
            app:destination="@id/addSongsToPlaylistsFragment" />
    </dialog>

</navigation>


